
.container {
	  background-color: #f5f5f5;
	  min-height: 100vh;
	  font-family: -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Helvetica, Arial, sans-serif;
}
.tips{
		font-size: 25rpx;
		color: gray; 
		margin-top: 10rpx;
}
.item{
		display: flex; 
		justify-content: center; 
		align-items: center;
}
.editButton{
		width: 120rpx;
		height: 80rpx;
		border-radius: 40rpx;
		background: rgba(255, 195, 0, 1);
		box-shadow: 0px 2px 4px  rgba(0, 0, 0, 0.25);
		display: flex;
		justify-content: center; 
		align-items: center;
}
.bt{
		width: 160rpx;
		height:80rpx;
		border-radius: 40px;
		background: rgba(255, 195, 0, 1);
		box-shadow: 0px 2px 4px  rgba(0, 0, 0, 0.25);
		display: flex;
		justify-content: center; 
		align-items: center;
}
.box{
		width: 160rpx;
		height: 140rpx;
		border-radius: 10rpx;
		background: rgba(255, 255, 255, 1);
		border: 3px solid rgba(255, 195, 0, 1);
		box-shadow: 0px 2px 4px  rgba(0, 0, 0, 0.25);
}
.itemContainer{
		padding-left: 30rpx; 
		padding-right: 30rpx;
		display: flex;
		flex-direction: column;
}
.containerVertical{
		display: flex; 
		flex-direction: column;
}
